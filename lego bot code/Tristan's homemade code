#include <kipr/wombat.h>
int bump1 = 0;
int bump2 = 1;
int rspeed = 970;
int lspeed = 1000;
int open = 1450;
int close = 535;
int pulleyspeed = 1000;
int oneeightyturn = 2210;
int ninetyturn = 1105;
int main()
{
    shut_down_in(120);
    mav(0,lspeed);
    mav(1,rspeed);
    msleep(2500);
    
    mav(0,lspeed);
    mav(1,-1*rspeed);
    msleep(ninetyturn);
    
    mav(0,lspeed);
    mav(1,rspeed);
    msleep(8000);
    
    mav(0,-1*lspeed);
    mav(1,rspeed);
    msleep(ninetyturn); //facing leftmost tower
    
      while((digital(bump1)==0)||(digital(bump2)==0))
      {
	mav(0,lspeed);
    	mav(1,rspeed);
    	msleep(10);
      }
    
    mav(0,-1*lspeed);
    mav(1,-1*rspeed);
    msleep(300);
    off(0);
    off(1); //at tower, both motors off
    
    enable_servo(0);
    set_servo_position(0,open);
    mav(2,-1*pulleyspeed);
    msleep(1300);
    off(2);
    set_servo_position(0,close);
    disable_servo(0);
    
    mav(2,pulleyspeed);
    msleep(1300);
    off(2);
    
    mav(0,-1*lspeed);
    mav(1,-1*rspeed);
    msleep(2500);
    
    mav(0,-1*lspeed);
    mav(1,rspeed);
    msleep(ninetyturn);
    
      while((digital(bump1)==0)||(digital(bump2)==0)) //bump next to rightmost tower
      {
	mav(0,lspeed);
    	mav(1,rspeed);
    	msleep(10);
      }
    
    mav(0,-1*lspeed);
    mav(1,-1*rspeed);
    msleep(2000);
    
    mav(0,-1*lspeed);
    mav(1,rspeed);
    msleep(ninetyturn);
    off(0);
    off(1);
    
    mav(2,-1*pulleyspeed);
    msleep(3000);
    off(2); //lower claw to drop cube
    
    enable_servo(0);
    set_servo_position(0,open); //first cube in cybersecurity lab
    mav(0,-1*lspeed);
    mav(1,rspeed);
    msleep(oneeightyturn);
    off(0);
    off(1);
    disable_servo(0);
    
    mav(2,pulleyspeed);
    msleep(3000);
    off(2);
    
    cmpc(0);
    msleep(10);
    cmpc(1;
    msleep(10);
   
    while((digital(bump1)==0)||(digital(bump2)==0)) //bump next to rightmost tower
	{
	  mav(0,lspeed);
    	  mav(1,rspeed);
    	  msleep(10);
	}
      
    mav(0,-1*lspeed);
    mav(1,-1*rspeed);
    msleep(300);
    off(0);
    off(1); //at tower, both motors off
    
    enable_servo(0);
    set_servo_position(0,open);
    mav(2,-1*pulleyspeed);
    msleep(1300);
    off(2);
    set_servo_position(0,close);
    disable_servo(0);
    
    mav(2,pulleyspeed);
    msleep(1300);
    off(2);
    
    gmpc(0);
    msleep(10);
    gmpc(1;
    msleep(10);
    
    mtp(0,-1*lspeed,0);
    mtp(1,-1*rspeed,0);
    msleep(10);
    
    mav(0,-1*lspeed);
    mav(1,rspeed);
    msleep(oneeightyturn);
    off(0);
    off(1);
    
    enable_servo(0);
    mav(2,-1*pulleyspeed);
    msleep(700);
    off(2);
    set_servo_position(0,open);
    disable_servo(0); //STACK HEIGHT 2!!!!!!
    
    mav(0,-1*lspeed);
    mav(1,rspeed);
    msleep(ninetyturn);
    
    
    
    return 0;
    }
